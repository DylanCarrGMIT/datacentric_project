<table>

    <tr>
        <th>Lecturer ID</th>
        <th>Name</th>
        <th>Dept</th>
        
    </tr>
    <% lecturers.forEach((lecturer) =>{ %>
        <tr>

            <td><%= lecturer._id %></td>
            <td><%= lecturer.name %></td>
            <td><%= lecturer.dept %></td>
        </tr>
    <% }) %>
</table>
